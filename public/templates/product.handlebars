{{#each products}}
    <div class="col-sm-4">
            <div class="panel panel-primary">
                <div class="panel-heading">{{this.title}}</div>
                <div class="panel-body"><img src="{{this.url}}" class="img-responsive" style="width:100%" alt="Image"></div>
                <div class="panel-footer"><small>Price: {{this.price}}$</small><button>Add to cart!</button></div>
            </div>
    </div>
{{/each}}